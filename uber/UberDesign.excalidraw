    {
      "id": "puCnF_e_NpUeGf1wwVSEL",
      "type": "text",
      "x": 467.48046875,
      "y": 3237.252685546875,
      "width": 880,
      "height": 150,
      "angle": 0,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "transparent",
      "fillStyle": "solid",
      "strokeWidth": 1,
      "strokeStyle": "solid",
      "roughness": 1,
